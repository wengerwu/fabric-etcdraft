order5个上面的msp
----------------------------------------
order5个上面的tls

fabric-ca-server init -b ordersca:123456 -u http://rootca:123456@localhost:7054 --home ./orderstls-server --cfg.affiliations.allowremove --cfg.identities.allowremove 


fabric-ca-client enroll -M crypto-config/ordererOrganizations/fabrichain.com/tlstmp -u http://ordersca:123456@localhost:8055 --home ca-client

fabric-ca-client affiliation list -M crypto-config/ordererOrganizations/fabrichain.com/tlstmp -u http://ordersca:123456@localhost:8055 --home ca-client
fabric-ca-client affiliation remove --force org1 -M crypto-config/ordererOrganizations/fabrichain.com/tlstmp -u http://ordersca:123456@localhost:8055 --home ca-client
fabric-ca-client affiliation remove --force org2 -M crypto-config/ordererOrganizations/fabrichain.com/tlstmp -u http://ordersca:123456@localhost:8055 --home ca-client
fabric-ca-client affiliation add com -M crypto-config/ordererOrganizations/fabrichain.com/tlstmp -u http://ordersca:123456@localhost:8055 --home ca-client
fabric-ca-client affiliation add com.fabrichain -M crypto-config/ordererOrganizations/fabrichain.com/tlstmp -u http://ordersca:123456@localhost:8055 --home ca-client

fabric-ca-client register --id.name Admin@fabrichain.com --id.type client --id.affiliation "com.fabrichain" --id.attrs '"hf.Registrar.Roles=client,orderer,peer,user","hf.Registrar.DelegateRoles=client,orderer,peer,user",hf.Registrar.Attributes=*,hf.GenCRL=true,hf.Revoker=true,hf.AffiliationMgr=true,hf.IntermediateCA=true,role=admin:ecert' --id.secret=123456 --csr.cn=fabrichain.com --csr.hosts=['fabrichain.com'] -M crypto-config/ordererOrganizations/fabrichain.com/tlstmp -u http://ordersca:123456@localhost:8055 --home ca-client


fabric-ca-client enroll -d --enrollment.profile tls -u http://Admin@fabrichain.com:123456@localhost:8055  --csr.cn=fabrichain.com --csr.hosts=['fabrichain.com'] -M crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/tlstmp --home ca-client

mkdir -p ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/tls

cp orderstls-server/ca-chain.pem ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/tls/ca.crt

cp ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/tlstmp/signcerts/cert.pem ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/tls/client.crt

mkdir -p ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/tlstmp/keystore

path=ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/tlstmp/keystore

keystore=`ls -l $path | tail -n 1 | awk '{print $9}'`

cp ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/tlstmp/keystore/$keystore ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/tls/client.key
cp -r ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/tlstmp/tlscacerts ca-client/crypto-config/ordererOrganizations/fabrichain.com/msp
cp -r ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/tlstmp/tlsintermediatecerts ca-client/crypto-config/ordererOrganizations/fabrichain.com/msp






fabric-ca-client register --id.name orderer.fabrichain.com --id.type orderer --id.affiliation "com.fabrichain" --id.attrs '"role=orderer",ecert=true' --id.secret=123456 --csr.cn=orderer.fabrichain.com --csr.hosts=['orderer.fabrichain.com'] -M crypto-config/ordererOrganizations/fabrichain.com/tlstmp -u http://ordersca:123456@localhost:8055 --home ca-client

fabric-ca-client enroll -d --enrollment.profile tls -u http://orderer.fabrichain.com:123456@localhost:8055 --csr.cn=orderer.fabrichain.com --csr.hosts=['orderer.fabrichain.com'] -M crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/tlstmp --home ca-client

mkdir -p ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/tls

cp orderstls-server/ca-chain.pem ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/tls/ca.crt

cp ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/tlstmp/signcerts/cert.pem ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/tls/server.crt

path=ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/tlstmp/keystore

keystore=`ls -l $path | tail -n 1 | awk '{print $9}'`

cp ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/tlstmp/keystore/$keystore ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/tls/server.key

cp -r ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/tlstmp/tlscacerts ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/msp

cp -r ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/tlstmp/tlsintermediatecerts  ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/msp





fabric-ca-client register --id.name orderer2.fabrichain.com --id.type orderer --id.affiliation "com.fabrichain" --id.attrs '"role=orderer",ecert=true' --id.secret=123456 --csr.cn=orderer2.fabrichain.com --csr.hosts=['orderer2.fabrichain.com'] -M crypto-config/ordererOrganizations/fabrichain.com/tlstmp -u http://ordersca:123456@localhost:8055 --home ca-client

fabric-ca-client enroll -d --enrollment.profile tls -u http://orderer2.fabrichain.com:123456@localhost:8055 --csr.cn=orderer2.fabrichain.com --csr.hosts=['orderer2.fabrichain.com'] -M crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer2.fabrichain.com/tlstmp --home ca-client

