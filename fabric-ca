mkdir rootca-server
fabric-ca-server init -b rootca:123456 --home ./rootca-server --cfg.affiliations.allowremove --cfg.identities.allowremove 

fabric-ca-server start -b rootca:123456 --home ./rootca-server --cfg.affiliations.allowremove --cfg.identities.allowremove 


fabric-ca-server init -b ordersca:123456 -u http://rootca:123456@localhost:7054 --home ./ordersca-server --cfg.affiliations.allowremove --cfg.identities.allowremove 



fabric-ca-client enroll -M crypto-config/ordererOrganizations/fabrichain.com/msp -u http://ordersca:123456@localhost:7055 --home ca-client

fabric-ca-client affiliation list -M crypto-config/ordererOrganizations/fabrichain.com/msp -u http://ordersca:123456@localhost:7055 --home ca-client


fabric-ca-client affiliation remove --force org1 -M crypto-config/ordererOrganizations/fabrichain.com/msp -u http://ordersca:123456@localhost:7055 --home ca-client


fabric-ca-client affiliation add com -M crypto-config/ordererOrganizations/fabrichain.com/msp -u http://ordersca:123456@localhost:7055 --home ca-client

fabric-ca-client affiliation add com.fabrichain -M crypto-config/ordererOrganizations/fabrichain.com/msp -u http://ordersca:123456@localhost:7055 --home ca-client


fabric-ca-client register --id.name Admin@fabrichain.com --id.type client --id.affiliation "com.fabrichain" --id.attrs '"hf.Registrar.Roles=client,orderer,peer,user","hf.Registrar.DelegateRoles=client,orderer,peer,user",hf.Registrar.Attributes=*,hf.GenCRL=true,hf.Revoker=true,hf.AffiliationMgr=true,hf.IntermediateCA=true,role=admin:ecert' --id.secret=123456 --csr.cn=fabrichain.com --csr.hosts=['fabrichain.com'] -M crypto-config/ordererOrganizations/fabrichain.com/msp -u http://ordersca:123456@localhost:7055 --home ca-client


fabric-ca-client enroll -u http://Admin@fabrichain.com:123456@localhost:7055 --csr.cn=fabrichain.com --csr.hosts=['fabrichain.com'] -M crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/msp --home ca-client


mkdir -p ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/msp/admincerts

cp ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/msp/signcerts/cert.pem ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/msp/admincerts

mkdir -p ca-client/crypto-config/ordererOrganizations/fabrichain.com/msp/admincerts

cp ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/msp/signcerts/cert.pem ca-client/crypto-config/ordererOrganizations/fabrichain.com/msp/admincerts

fabric-ca-client register --id.name orderer.fabrichain.com --id.type orderer --id.affiliation "com.fabrichain" --id.attrs '"role=orderer",ecert=true' --id.secret=123456 --csr.cn=orderer.fabrichain.com --csr.hosts=['orderer.fabrichain.com'] -M crypto-config/ordererOrganizations/fabrichain.com/msp -u http://ordersca:123456@localhost:7055 --home ca-client

fabric-ca-client enroll -u http://orderer.fabrichain.com:123456@localhost:7055 --csr.cn=orderer.fabrichain.com --csr.hosts=['orderer.fabrichain.com'] -M crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/msp --home ca-client


mkdir -p ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/msp/admincerts

cp ca-client/crypto-config/ordererOrganizations/fabrichain.com/users/Admin@fabrichain.com/msp/signcerts/cert.pem ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer.fabrichain.com/msp/admincerts



fabric-ca-client register --id.name orderer2.fabrichain.com --id.type orderer --id.affiliation "com.fabrichain" --id.attrs '"role=orderer",ecert=true' --id.secret=123456 --csr.cn=orderer2.fabrichain.com --csr.hosts=['orderer2.fabrichain.com'] -M crypto-config/ordererOrganizations/fabrichain.com/msp -u http://ordersca:123456@localhost:7055 --home ca-client


