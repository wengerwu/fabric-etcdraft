keystore=`ls -l $path | tail -n 1 | awk '{print $9}'`

cp ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer2.fabrichain.com/tlstmp/keystore/$keystore ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer2.fabrichain.com/tls/server.key

cp -r ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer2.fabrichain.com/tlstmp/tlscacerts ca-client/crypto-config/ordererOrganizations/fabrichain.com/orderers/orderer2.fabrichain.com/msp

